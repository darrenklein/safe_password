{
  "name": "Safe Password",
  "version": "0.1.0",
  "description": "",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/passwords.js", "content/binary_search.js", "content/content.js"]
    }
  ],
  "permissions": ["activeTab", "notifications"]
}
